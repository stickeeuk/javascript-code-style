    "js-beautify": "^1.12.0",
    "ora": "^5.0.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/js-beautify": "^1.11.0",
    "@types/node": "^14.0.27",
    "cz-conventional-changelog": "^3.2.0",
    "semantic-release": "^19.0.3",
    "typescript": "^3.9.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
